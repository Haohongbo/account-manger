{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "账号管理系统",
    "version": "1.0.0",
    "identifier": "com.example.account-manager",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "账号管理系统",
                "width": 1200,
                "height": 800,
                "resizable": true,
                "fullscreen": false,
                "center": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": []
    }
}